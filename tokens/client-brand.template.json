{
  "color": {
    "primary": "#your-primary-color",
    "primaryLight": "#your-primary-light-color",
    "accent": "#your-accent-color",
    "success": "#your-success-color",
    "neutral": {
      "50": "#your-neutral-50",
      "100": "#your-neutral-100",
      "600": "#your-neutral-600",
      "900": "#your-neutral-900"
    }
  },
  "font": {
    "family": {
      "heading": "'Your Heading Font', serif",
      "body": "'Your Body Font', sans-serif"
    },
    "scale": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem"
    }
  },
  "radius": {
    "sm": "0.25rem",
    "md": "0.375rem",
    "lg": "0.5rem",
    "xl": "0.75rem"
  },
  "shadow": {
    "card": "0 4px 12px rgba(your-primary-rgb, 0.15)",
    "elevated": "0 8px 24px rgba(your-primary-rgb, 0.2)"
  },
  "spacing": {
    "xs": "0.5rem",
    "sm": "0.75rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem"
