{
  "color": {
    "primary": "#293919",
    "primary-light": "#3d5226",
    "primary-dark": "#1a2410",
    "white": "#ffffff",
    "black": "#000000",
    "gray-50": "#f9fafb",
    "gray-100": "#f3f4f6",
    "gray-200": "#e5e7eb",
    "gray-300": "#d1d5db",
    "gray-600": "#4b5563",
    "gray-800": "#1f2937",
    "gray-900": "#111827",
    "success": "#10b981",
    "warning": "#f59e0b",
    "error": "#ef4444",
    "accent": "#3d5226"
  },
  "font": {
    "family": {
      "body": "'Source Sans Pro', -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "heading": "'Libre Baskerville', Georgia, serif"
    },
    "weight": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "size": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem"
    }
  },
  "radius": {
    "sm": "0.25rem",
    "md": "0.375rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "pill": "9999px"
  },
  "shadow": {
    "card": "0 4px 12px rgba(41, 57, 25, 0.15)",
    "elevated": "0 4px 12px rgba(41, 57, 25, 0.25)"
  },
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem"
  },
  "button": {
    "primary": {
      "bg": "#293919",
      "text": "#ffffff",
      "hoverBg": "#3d5226"
    },
    "secondary": {
      "bg": "transparent",
      "text": "#293919",
      "border": "#293919"
    }
  },
  "transition": {
    "fast": "150ms ease-in-out",
    "base": "250ms ease-in-out"
  }
}
